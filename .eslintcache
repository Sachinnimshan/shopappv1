y) ? (<LoadingBox></LoadingBox>):\r\n                                (\r\n                                <>\r\n                                {errorpay && (<MessageBox>{errorpay}</MessageBox>)}\r\n                                {loadingpay && (<LoadingBox></LoadingBox>)}\r\n                                <PayPalButton amount={order.TotalPrice}\r\n                                onSuccess={SuccessPaymentHandler}></PayPalButton>\r\n                                </>)}\r\n                        </div>\r\n                        )} \r\n                </div>\r\n            </div> \r\n        </div>)\r\n    )\r\n}\r\n\r\nexport default OrderDetailsView;\r\n","G:\\shopamz\\shopappv1\\src\\Reducers\\OrderHistoryReducer.js",[],"G:\\shopamz\\shopappv1\\src\\PlaceOrder\\OrderHistory.js",[],"G:\\shopamz\\shopappv1\\src\\Actions\\OrderHistoryActions.js",[],"G:\\shopamz\\shopappv1\\src\\Profile\\UserProfile.js",[],"G:\\shopamz\\shopappv1\\src\\Reducers\\UserProfileReducer.js",[],"G:\\shopamz\\shopappv1\\src\\Profile\\PrivateRoute.js",[],"G:\\shopamz\\shopappv1\\src\\AdminPanel\\AdminHome.js",["208","209","210","211","212","213","214","215","216","217","218","219"],"G:\\shopamz\\shopappv1\\src\\AdminPanel\\AdminProductView.js",["220","221","222"],{"ruleId":"223","replacedBy":"224"},{"ruleId":"225","replacedBy":"226"},{"ruleId":"227","severity":1,"message":"228","line":35,"column":30,"nodeType":"229","endLine":35,"endColumn":86},{"ruleId":"230","severity":1,"message":"231","line":1,"column":8,"nodeType":"232","messageId":"233","endLine":1,"endColumn":13},{"ruleId":"230","severity":1,"message":"231","line":1,"column":8,"nodeType":"232","messageId":"233","endLine":1,"endColumn":13},{"ruleId":"230","severity":1,"message":"234","line":4,"column":8,"nodeType":"232","messageId":"233","endLine":4,"endColumn":13},{"ruleId":"235","severity":1,"message":"236","line":17,"column":8,"nodeType":"237","endLine":17,"endColumn":10,"suggestions":"238"},{"ruleId":"223","replacedBy":"239"},{"ru